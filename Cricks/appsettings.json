{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    },
    "AzureAppServicesFile": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft": "Warning"
      }
    },
    "AzureAppServicesBlob": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft": "Warning"
      },
      "BlobName": "log.txt"
    }
  },

  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=cricks.db"
  },
  "AppSettings": {
    "Token": "CricksSecretKeyForTokenGeneration"
  }
}
